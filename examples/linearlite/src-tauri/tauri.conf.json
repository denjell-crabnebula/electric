{
  "productName": "linearlite",
  "version": "0.0.0",
  "identifier": "com.linearlite.dev",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1337",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {"windows": [
      {
        "title": "Linearlite",
        "width": 1200,
        "height": 800,
        "alwaysOnTop": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "iOS": {
      "developmentTeam": "demo"
    }
  }
}
